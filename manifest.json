{
  "name": "Link Redirect",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Redirect download links to autogeneratelink.com",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }],
  "browser_action": {
    "default_icon": "icon.png",
	"default_popup": "popup.html"
  }
  
  
}